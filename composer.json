{
    "name": "daun/processwire-image-placeholders",
    "description": "A ProcessWire module to generate low-quality image placeholders (LQIP) for smoother lazyloading",
    "license": "MIT",
    "type": "processwire-module",
    "keywords": [
        "processwire",
        "image",
        "placeholder",
        "lqip",
        "thumbhash",
        "blurhash",
        "lazyload"
    ],
    "authors": [
        {
            "name": "Philipp Daun",
            "email": "post@philippdaun.net",
            "homepage": "https://philippdaun.net"
        }
    ],
    "homepage": "https://github.com/daun/processwire-image-placeholders",
    "require": {
        "php": ">=8.2",
        "ext-exif": "*",
        "ext-gd": "*",
        "composer/installers": "^1.0",
        "kornrunner/blurhash": "^1.2",
        "srwiez/thumbhash": "^1.2"
    },
    "require-dev": {
        "rector/rector": "^2.1"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "extra": {
        "installer-name": "ImagePlaceholders"
    }
}
